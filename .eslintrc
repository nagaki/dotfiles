{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "camelcase": 0,
    "curly": [2, "all"],
    "indent": [2, 2],
    "quotes": [2, "double"],
    "react/jsx-uses-vars": 1,
    "strict": 0,
    "no-comma-dangle": 2,
    "no-extra-boolean-cast": 1,
    "no-redeclare": 1,
    "no-underscore-dangle": 0
  }
}
